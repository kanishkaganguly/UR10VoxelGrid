<?xml version='1.0'?>
<sdf version="1.4">
    <model name="obstacle_box_small">
        <pose>0.05 0.05 0.05 0 0 0</pose>
        <static>false</static>
        <link name="obstacle_small_link">
            <inertial>
                <mass>0.5</mass>
                <inertia>
                    <ixx>0.0002</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
                    <ixy>0.0</ixy>          <!-- for a box: ixy = 0 -->
                    <ixz>0.0</ixz>          <!-- for a box: ixz = 0 -->
                    <iyy>0.0002</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
                    <iyz>0.0</iyz>          <!-- for a box: iyz = 0 -->
                    <izz>0.0002</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
                </inertia>
            </inertial>
            <collision name="obstacle_small_collision">
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
            </collision>
            <visual name="obstacle_small_visual">
                <geometry>
                    <box>
                        <size>0.05 0.05 0.05</size>
                    </box>
                </geometry>
            </visual>
            <sensor name="obstacle_small_contact_sensor" type="contact">
                <always_on>1</always_on>
                <update_rate>5</update_rate>
                <contact>
                    <collision>obstacle_small_collision</collision>
                    <topic>/obstacle_small_contact</topic>
                </contact>
            </sensor>
        </link>
    </model>
</sdf>